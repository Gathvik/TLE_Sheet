#include <bits/stdc++.h>
using namespace std;
#define forn(i, a, n) for (long long i = a; i < n; i++)
#define int long long
#define endl '\n'

// const int MAXN = 200100;
// const int MOD = 1000000007;

void solve() {
  int n, x;
  cin >> n >> x;
  vector<int> b(n);
  forn(i, 0, n) cin >> b[i];
  vector<int> a(n + 1);
  forn(i, 0, n - 1) a[i] = b[i + 1] - b[i];
  a[n - 1] = 2 * (x - b[n - 1]);
  a[n] = b[0] - 0;
  sort(a.begin(), a.end());
  cout << a[n] << endl;
}

int32_t main() {
  ios::sync_with_stdio(false);
  cin.tie(0);
  cout.tie(0);
  int T = 1;
  cin >> T; // Comment this out in case number of test cases are not to be
            // taken as input
  for (int I = 1; I <= T; I++) {
    solve();
  }
  return 0;
}
